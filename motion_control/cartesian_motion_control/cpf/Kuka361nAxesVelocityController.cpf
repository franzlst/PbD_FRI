<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "cpf.dtd">
<properties>
  <struct name="driveLimits" type="array">
     <description>velocity limits of the axes, (rad/s)</description>
     <simple name="0" type="double"><value>1</value></simple>
     <simple name="1" type="double"><value>1</value></simple>
     <simple name="2" type="double"><value>1</value></simple>
     <simple name="3" type="double"><value>1</value></simple>
     <simple name="4" type="double"><value>1</value></simple>
     <simple name="5" type="double"><value>1</value></simple>
  </struct>
  <struct name="LowerPositionLimits" type="array">
     <description>Lower position limits (rad)</description>
     <simple name="0" type="double"><value>-30</value></simple>
     <simple name="1" type="double"><value>-30</value></simple>
     <simple name="2" type="double"><value>-30</value></simple>
     <simple name="3" type="double"><value>-30</value></simple>
     <simple name="4" type="double"><value>-30</value></simple>
     <simple name="5" type="double"><value>-30</value></simple>
  </struct>
  <struct name="UpperPositionLimits" type="array">
     <description>Upper position limits (rad)</description>
     <simple name="0" type="double"><value>30</value></simple>
     <simple name="1" type="double"><value>30</value></simple>
     <simple name="2" type="double"><value>30</value></simple>
     <simple name="3" type="double"><value>30</value></simple>
     <simple name="4" type="double"><value>30</value></simple>
     <simple name="5" type="double"><value>30</value></simple>
  </struct>
  <struct name="initialPosition" type="array">
     <description>Initial position (rad) for simulation or hardware</description>
     <simple name="0" type="double"><value>0</value></simple>
     <simple name="1" type="double"><value>0</value></simple>
     <simple name="2" type="double"><value>0</value></simple>
     <simple name="3" type="double"><value>0</value></simple>
     <simple name="4" type="double"><value>0</value></simple>
     <simple name="5" type="double"><value>0</value></simple>
  </struct>
  <struct name="driveOffset" type="array">
     <description>offset (in rad/s) to the drive value.</description>
     <simple name="0" type="double"><value>0</value></simple>
     <simple name="1" type="double"><value>0</value></simple>
     <simple name="2" type="double"><value>0</value></simple>
     <simple name="3" type="double"><value>0</value></simple>
     <simple name="4" type="double"><value>0</value></simple>
     <simple name="5" type="double"><value>0</value></simple>
  </struct>
  <simple name="simulation" type="boolean"><description>true if simulationAxes should be used</description><value>1</value></simple>
  <simple name="geometric" type="boolean"><description>true if geometric values should be used</description><value>1</value></simple>
  <struct name="EmergencyEvents" type="list">
     <description>List of events that will result in an emergencystop of the robot</description>
     <simple name="Event 1" type="string"><description></description><value>Robot.driveOutOfRange</value></simple>
   </struct>
</properties>
